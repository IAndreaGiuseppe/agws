{
    "name": "iag/agws",
    "description": "andreagiuseppe.com website",
    "homepage": "https://github.com/IAndreaGiuseppe/agws",
    "support": {
        "issues": "https://github.com/IAndreaGiuseppe/agws/issues",
        "source": "https://github.com/IAndreaGiuseppe/agws"
    },
    "authors": [
        {
            "name": "Andrea Giuseppe Pretto",
            "email": "info@andreagiuseppe.com",
            "homepage": "https://andreagiuseppe.com"
        }
    ],
    "require": {
        "iag/metadata": "^0.2.0",
        "illuminate/routing": "^7.0",
        "illuminate/support": "^7.0",
        "php": "^7.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Iag\\Agws\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iag\\Agws\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iag\\Agws\\AgwsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
